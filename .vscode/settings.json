{
    "python.formatting.provider": "autopep8",
    "python.linting.flake8Enabled": false,
    "python.linting.enabled": false,
    "python.linting.pylintArgs": [
        "--disable-msg=C0103"
    ],
    "python.pythonPath": "python3",
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "*.pyc": true,
        "*.pyo": true,
        "__pycache__": true
    }
}